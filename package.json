{
  "name": "expandable-tree",
  "version": "0.2.0",
  "description": "Expandable Tree React Component",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && ./node_modules/webpack/bin/webpack.js",
    "dev": "./node_modules/webpack/bin/webpack.js --watch",
    "clean": "rm -rf dist",
    "dist": "npm run clean && npm run build",
    "examples": "npm run build && ./node_modules/webpack/bin/webpack.js --config webpack.examples.js",
    "examples-dev": "./node_modules/webpack/bin/webpack.js --config webpack.examples.js --watch",
    "prepublish": "npm run dist",
    "test": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iLoveCodingOrg/expandable-tree.git"
  },
  "keywords": [
    "react-component",
    "expandable-tree",
    "treeview",
    "tree",
    "expand",
    "expandable",
    "collapsable",
    "collapse"
  ],
  "author": "Aziz Ali <aziz@ilovecoding.org>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react-transition-group": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "react": "^16.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-node-externals": "^1.6.0"
  }
}
